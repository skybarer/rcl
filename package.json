{
  "name": "@skybarer/rcl",
  "version": "0.1.2",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skybarer/rcl"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build --stats",
    "analyze": "webpack-bundle-analyzer build/bundle-stats.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src/atoms --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-a11y": "^6.4.16",
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/addon-viewport": "^6.4.16",
    "@storybook/builder-webpack5": "^6.4.16",
    "@storybook/manager-webpack5": "^6.4.16",
    "@storybook/node-logger": "^6.4.16",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.16",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "postcss": "^8.4.6",
    "postcss-modules": "^4.3.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3"
  }
}
